n":15},"generated":{"line":1,"column":3463}},{"source":"pages/Introduction.js","name":"Introduction","original":{"line":47,"column":15},"generated":{"line":1,"column":3471}},{"source":"pages/Introduction.js","name":"Introduction","original":{"line":47,"column":15},"generated":{"line":1,"column":3479}}],"sources":{"pages/Introduction.js":"import React from 'react'\n\nconst Introduction = () => {\n  return (\n    <div className=\"row\">\n      <div className='col-sm-12' style={{ backgroundImage: 'url(http://theme.hstatic.net/1000384235/1000541562/14/welcome_bg.jpg?v=263)', opacity: 'cover', padding: '210px 0 210px 0' }}>\n        <div className='col-sm-5' style={{ float: \"right\", marginRight: '200px' }}>\n          <h1 style={{ fontSize: '75px' }}>Welcome to</h1>\n          <div style={{ display: 'flex' }}>\n            <p style={{ fontSize: '55px', fontWeight: '500', color: '#30942A' }}>FruiShop</p>\n            <p style={{ marginLeft: '46px', fontSize: '22px', color: '#000', marginTop: '17px' }}>Tươi ngon tới tận cửa nhà\n            Giao hàng nhanh trong 2H\n            </p>\n          </div>\n        </div>\n      </div>\n\n      <div className='col-sm-12' style={{ backgroundImage: 'url(http://theme.hstatic.net/1000384235/1000541562/14/connect_bg.jpg?v=263)', opacity: 'cover', padding: '120px 0 120px 0' }}>\n        <div className='col-sm-5' style={{ float: \"left\", marginLeft: '150px' }}>\n          <h1 style={{ color: '#30942A', fontWeight: 'bold', fontSize: '45px' }}>FruiShop</h1>\n          <p style={{ fontWeight: 'bold', fontSize: '26px' }}>Nền tảng đầu tiên kết nối trực tiếp nhà sản xuất với người tiêu dùng</p>\n          <p style={{ color: '#000', fontSize: '18px' }}>\n            FruiShop ra đời nhằm giải quyết tất thày các vấn đề hạn chế của quy trình sản xuất và cung cấp nông sản Việt.\n            Bằng việc số hóa từng loại nông sản trên nền tảng công nghệ, F99 đã tạo nên các gian hàng thương mại điện tử cho nông sản Việt,\n            cung cấp các sản phẩm đảm bảo chất lượng, an toàn cho sức khỏe. Ngược lại, thông qua F99,\n            các hộ nông sản và nhà sản xuất có thê tự tìm đầu ra cho sản phẩm,\n            từ đó gây dựng được thương hiệu của riêng mình.\n          </p>\n        </div>\n      </div>\n\n      <div className='col-sm-12' style={{ backgroundImage: 'url(http://theme.hstatic.net/1000384235/1000541562/14/commitment_bg.jpg?v=263)', opacity: 'cover', padding: '105px 0 105px 0' }}>\n        <div className='col-sm-5' style={{ float: \"right\", marginRight: '190px' }}>\n          <h1 style={{ color: '#30942A', fontWeight: 'bold', fontSize: '45px' }}>Chọn FruiShop - Chọn cuộc sống xanh - Chọn tiện ích lớn</h1>\n          <p style={{ color: '#000', fontSize: '18px', fontWeight: '400' }} >MUA SẮM TIỆN LỢI - MỌI LÚC MỌI NƠI</p>\n          <p style={{ color: '#000', fontSize: '18px', fontWeight: '400' }}>+ Đơn giản, an toàn, đa dạng</p>\n          <p style={{ color: '#000', fontSize: '18px', fontWeight: '400' }}>ƯU ĐÃI ĐỘC QUYỀN - TIẾT KIỆM TỐI ĐA</p>\n          <p style={{ color: '#000', fontSize: '18px', fontWeight: '400' }}>+ Giá rẻ bất ngờ, voucher ưu đãi</p>\n          <p style={{ color: '#000', fontSize: '18px', fontWeight: '400' }}>QUY TRÌNH TỐI ƯU - GIAO HÀNG NHANH CHÓNG</p>\n          <p style={{ color: '#000', fontSize: '18px', fontWeight: '400' }}>+ Giao hàng toàn quốc, hỗ trợ 24/7</p>\n          <p style={{ color: '#000', fontSize: '18px', fontWeight: '400' }}>Với tất cả các ưu thế về sản phẩm và dịch vụ đi đầu, FruiShop đã và đang ngày một phát triển lớn mạnh hơn nữa để có thể đáp ứng kỳ vọng của khách hàng toàn quốc, đem tới chất lượng cuộc sống ngày một tốt hơn cho người dân Việt</p>\n        </div>\n      </div>\n    </div>\n  )\n}\nexport default Introduction"},"lineCount":null}},"error":null,"hash":"3e6252fc7608f914161ca5b0b0e58782","cacheData":{"env":{}}}